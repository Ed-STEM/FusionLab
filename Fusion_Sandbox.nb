Cell["Graphical Representation of Simple Plasma Closed Field Loop ", \
"Subsection",ExpressionUUID->"862007dd-4f7f-4e95-83ee-0287e7d1b5ec"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", "Parameters", "*)"}], 
  RowBox[{
   RowBox[{
    RowBox[{"mu0", "=", 
     RowBox[{"4", " ", "Pi", "*", 
      RowBox[{"10", "^", 
       RowBox[{"-", "7"}]}]}]}], ";"}], " ", 
   RowBox[{"(*", 
    RowBox[{"Permeability", " ", "of", " ", "free", " ", "space"}], "*)"}], 
   "\n", 
   RowBox[{
    RowBox[{"current", "=", "1"}], ";"}], " ", 
   RowBox[{"(*", 
    RowBox[{"Current", " ", "in", " ", "the", " ", "loop"}], "*)"}], "\n", 
   RowBox[{
    RowBox[{"radius", "=", "1"}], ";"}], " ", 
   RowBox[{"(*", 
    RowBox[{"Radius", " ", "of", " ", "the", " ", "plasma", " ", "loop"}], 
    "*)"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{
     "Function", " ", "to", " ", "compute", " ", "magnetic", " ", "field", 
      " ", "using", " ", "Biot"}], "-", 
     RowBox[{"Savart", " ", "Law"}]}], "*)"}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"BiotSavartLaw", "[", "point_", "]"}], ":=", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"dl", ",", "rVec", ",", "dB"}], "}"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"dl", "[", "\[Phi]_", "]"}], ":=", 
         RowBox[{"R", " ", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"-", 
             RowBox[{"Sin", "[", "\[Phi]", "]"}]}], ",", 
            RowBox[{"Cos", "[", "\[Phi]", "]"}], ",", "0"}], "}"}]}]}], ";", 
        RowBox[{"(*", 
         RowBox[{"Differential", " ", "length", " ", "element"}], "*)"}], 
        RowBox[{
         RowBox[{"rVec", "[", "\[Phi]_", "]"}], ":=", 
         RowBox[{"point", "-", 
          RowBox[{"R", " ", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"Cos", "[", "\[Phi]", "]"}], ",", 
             RowBox[{"Sin", "[", "\[Phi]", "]"}], ",", "0"}], "}"}]}]}]}], 
        ";", 
        RowBox[{"(*", 
         RowBox[{"Position", " ", "vector"}], "*)"}], 
        RowBox[{
         RowBox[{"dB", "[", "\[Phi]_", "]"}], ":=", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"mu0", " ", "I"}], ")"}], "/", 
           RowBox[{"(", 
            RowBox[{"4", " ", "Pi"}], ")"}]}], " ", 
          RowBox[{
           RowBox[{"Cross", "[", 
            RowBox[{
             RowBox[{"dl", "[", "\[Phi]", "]"}], ",", 
             RowBox[{"rVec", "[", "\[Phi]", "]"}]}], "]"}], "/", 
           RowBox[{
            RowBox[{"Norm", "[", 
             RowBox[{"rVec", "[", "\[Phi]", "]"}], "]"}], "^", "3"}]}]}]}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{"NIntegrate", "[", 
         RowBox[{
          RowBox[{"dB", "[", "\[Phi]_", "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"\[Phi]", ",", "0", ",", 
            RowBox[{"2", " ", "Pi"}]}], "}"}]}], "]"}]}]}], "]"}]}], ";"}], 
   "\n", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Compute", " ", "and", " ", "visualize", " ", "the", " ", "magnetic", " ",
      "field", " ", "at", " ", "various", " ", "points"}], "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"points", "=", 
     RowBox[{"Flatten", "[", 
      RowBox[{
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"x", ",", "y", ",", "0"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", 
           RowBox[{"-", "2"}], ",", "2", ",", "0.5"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"y", ",", 
           RowBox[{"-", "2"}], ",", "2", ",", "0.5"}], "}"}]}], "]"}], ",", 
       "1"}], "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"fields", "=", 
     RowBox[{"BiotSavartLaw", "/@", "points"}]}], ";"}], "\n", 
   RowBox[{"Graphics3D", "[", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"Arrowheads", "[", "0.02", "]"}], ",", 
      RowBox[{
       RowBox[{
        RowBox[{"Arrow", "[", 
         RowBox[{"Tube", "[", 
          RowBox[{"{", 
           RowBox[{"#1", ",", 
            RowBox[{"#1", "+", 
             RowBox[{"0.2", " ", "#2"}]}]}], "}"}], "]"}], "]"}], "&"}], "@@@", 
       RowBox[{"Transpose", "[", 
        RowBox[{"{", 
         RowBox[{"points", ",", "fields"}], "}"}], "]"}]}]}], "}"}], "]"}], 
   "\n", "\n"}]}]], "Input",
 CellLabel->"In[15]:=",ExpressionUUID->"3d3096be-a726-4747-8a79-8611af8f55d1"],

